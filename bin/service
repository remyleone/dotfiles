# il faut récupérer la variable $SERVICE_DIR
. /usr/local/scripts/kc.profile

# wrapper de gestion des services

for i in `echo $SERVICE_DIR` ; do
        if [ -e $i/$1 ] ; then
                $i/$*
                break
        fi
done

